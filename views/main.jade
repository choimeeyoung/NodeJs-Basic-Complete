doctype html
html
    head
        meta(charset='utf-8')
    body
        h1
            a(href='/topic')='Hello ListWorld'
        article
            ul
                each title in titles
                    li
                        a(href='/topic/'+title)= title

        article
            h2=title
            p=description

        article
            a(href='/topic/form')='Go Write'
